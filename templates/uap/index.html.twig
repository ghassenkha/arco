{% extends 'base.html.twig' %}

{% block title %}Uap index{% endblock %}

{% block body %}
    <h1>Uap index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Poste</th>
                <th>Section</th>
                <th>eff</th>
                <th>ineff</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for uap in uaps %}
            <tr>
                <td>{{ uap.id }}</td>
                <td>{{ uap.poste }}</td>
                <td>{{ uap.section }}</td>
                <td>{{ uap.eff }}</td>
                <td>{{ uap.ineff }}</td>
                <td>
                    <a href="{{ path('uap_show', {'id': uap.id}) }}">show</a>
                    <a href="{{ path('uap_edit', {'id': uap.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('uap_new') }}">Create new</a>
{% endblock %}
