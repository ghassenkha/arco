{% extends 'base.html.twig' %}

{% block title %}Production index{% endblock %}

{% block body %}
    <h1>Production index</h1>

    <table id="example" class="table">
        <thead>
            <tr>
                <th>No</th>
                <th>Qt</th>
                <th>Stock</th>
            </tr>
        </thead>
        <tbody>
        {% for production in productions %}
            <tr>
                <td>{{ production.no }}</td>
                <td>{{ production.qt }}</td>
                <td>{{ production.stock }}</td>

            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('production_new') }}">Create new</a>
{% endblock %}
